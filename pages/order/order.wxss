page{background-color:#f9f8f9;}
.nav{width:100%;height:100rpx;display:flex;flex-direction:row;}
.default{line-height:100rpx;text-align:center;flex:1;border-right:none;color:#5d5d5d;font-size:28rpx;}
.red{line-height:100rpx;text-align:center;color:#66CC99;flex:1;border-right:none; border-bottom: 4rpx solid #66CC99;font-size:28rpx;}
.show{display:block;background:#f7f7f7;}
.hidden{display:none;text-align:center;line-height:200px;}
.body{
  height: 100%;
}
.cardList {
  background: white;
  margin: 20rpx 0;
  border-bottom: solid 1px #dedede;
  border-top: solid 1px #dedede;
}
.cardList .head {
  font-size: 25rpx;
  border-bottom: solid 1px #dedede;
  color:#5d5d5d;
  padding: 20rpx;
}
.cardList .body {
  padding: 20rpx;
  border-bottom: solid 1px #dedede;
}

.cardList .body .photo {
  display: flex;
  align-items: center;
}
.cardList .body .photo .type {
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left: 15rpx;
}
.cardList .body .photo image{
    height:100rpx;
    width:100rpx;
    border-radius:50%;
    overflow:hidden;

}
.cardList .body .msg {
  margin-top: 10rpx;
  padding: 20rpx;
  font-size: 28rpx;
  color: #5d5d5d;
}
.cardList .footer {
  align-items: center;
  padding: 20rpx;
}
.span {
  display: inline-block;
  font-size: 18rpx;
  background: #66CC99;
  color: white;
  padding: 5rpx;
  border-radius: 5rpx;
  
}